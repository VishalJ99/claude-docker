{
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "86400000",
    "BASH_MAX_TIMEOUT_MS": "86400000",
    "MCP_TIMEOUT_MS": "60000"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Read(~/.zshrc)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(awk:*)",
      "Bash(sed:*)"
    ]
  }
}
